      <Compile Update="MachineStructure\TransitionResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>TransitionResources.resx</DependentUpon>
      </Compile>
    </ItemGroup>

    <ItemGroup>
      <EmbeddedResource Update="MachineDetails\MachineDetailsAggregateRootResources.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>MachineDetailsAggregateRootResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="MachineNameResources.resx">
        <Generator>ResXFileCodeGenerator</Generator>
