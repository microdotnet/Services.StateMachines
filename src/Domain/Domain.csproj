        <AutoGen>True</AutoGen>
        <DependentUpon>MachineDefinitionAggregateRootResources.resx</DependentUpon>
      </Compile>
      <Compile Update="MachineStructure\NodeResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>NodeResources.resx</DependentUpon>
      </Compile>
      <Compile Update="MachineStructure\StatusResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>StatusResources.resx</DependentUpon>
