      </EmbeddedResource>
      <EmbeddedResource Update="MachineStructure\NodeResources.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>NodeResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="MachineStructure\StatusResources.resx">
        <Generator>ResXFileCodeGenerator</Generator>
        <LastGenOutput>StatusResources.Designer.cs</LastGenOutput>
      </EmbeddedResource>
      <EmbeddedResource Update="MachineStructure\TransitionResources.resx">
