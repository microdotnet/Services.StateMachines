        <Nullable>enable</Nullable>
        <AssemblyName>MicroDotNet.Services.StateMachines.Domain</AssemblyName>
        <RootNamespace>MicroDotNet.Services.StateMachines.Domain</RootNamespace>
        <Company>MicroDotNet</Company>
        <Product>MicroDotNet - StateMachines</Product>
    </PropertyGroup>

    <ItemGroup>
      <Compile Update="MachineDetails\MachineDetailsAggregateRootResources.Designer.cs">
        <DesignTime>True</DesignTime>
        <AutoGen>True</AutoGen>
        <DependentUpon>MachineDetailsAggregateRootResources.resx</DependentUpon>
      </Compile>
      <Compile Update="MachineNameResources.Designer.cs">
        <DesignTime>True</DesignTime>
